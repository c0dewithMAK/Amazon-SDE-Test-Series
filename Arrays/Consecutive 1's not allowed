// { Driver Code Starts
#include <bits/stdc++.h>

using namespace std;
#define ll long long

 // } Driver Code Ends
//User function template for C++
class Solution{
public:
    int mod = 1e9 + 7;
	// #define ll long long
	ll countStrings(int n) {
	    if( n == 1 )
	        return 2;
	        
	    // this array represants count of (i+1)th length strings who end with '0' 
	    ll a[n] = {0};
	    // this array represants count of (i+1)th length strings who end with '1'
	    ll b[n] = {0};
	    a[0] = 1;
	    b[0] = 1;
	    
	    for(int i = 1; i < n; ++i){
	        a[i] = (a[i - 1] + b[i - 1]) % mod;
            b[i] = a[i - 1] % mod;
	    }
	    
	    return (a[n-1] + b[n-1]) % mod;
	}
};

// { Driver Code Starts.

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        Solution ob;
        auto ans = ob.countStrings(n);
        cout << ans << "\n";
    }
    return 0;
}  // } Driver Code Ends
